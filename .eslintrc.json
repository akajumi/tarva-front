{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": "react-app",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true,
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "single"],
    "semi": ["error", "never"],
    "react/jsx-uses-vars": ["error"],
    "no-console": ["off"]
  },
  "globals": {
    "process": false
  }
}